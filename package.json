{
  "name": "electron-vue-vite",
  "version": "0.0.0",
  "description": "是基于 `Electron` + `Vue` + `Vite` 创建的项目模板。",
  "private": true,
  "main": "app/electron/main.js",
  "author": "Aurora-flower",
  "scripts": {
    "dev": "vite",
    "up": "npm update --major",
    "check:out": "npm outdated",
    "check:audit": "npm audit --registry=https://registry.npmjs.org",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build": "node build/buildSetup.js",
    "build-only": "vite build",
    "build-check": "run-p type-check \"build-only {@}\" --",
    "type-check": "vue-tsc --build",
    "lint": "eslint .",
    "format": "prettier --write source/",
    "prepare": "husky",
    "clean:cache": "npm cache clean --force",
    "clean:app": "rimraf app",
    "commit": "node scripts/commit.js",
    "clean:release": "rimraf release",
    "package": "run-p \"clean:release\" \"build\" && electron-builder"
  },
  "dependencies": {
    "electron-log": "^5.3.2",
    "pinia": "^3.0.1",
    "rimraf": "^6.0.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@tailwindcss/postcss": "^4.0.15",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.11",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vitest/eslint-plugin": "^1.1.36",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "electron": "^35.0.0",
    "electron-builder": "^25.1.8",
    "eslint": "^9.23.0",
    "eslint-plugin-vue": "~10.0.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.5.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "3.5.3",
    "sass": "^1.86.0",
    "tailwindcss": "^4.0.14",
    "terser": "^5.39.0",
    "typescript": "~5.8.0",
    "vite": "^6.2.1",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-native": "^2.2.2",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.0.8",
    "vue-tsc": "^2.2.8"
  }
}
